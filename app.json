{
  "expo": {
    "name": "NovaRemote",
    "slug": "novaremote",
    "version": "1.1.0",
    "scheme": "novaremote",
    "orientation": "portrait",
    "icon": "./assets/novaai-logo-user.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/novaai-logo-user.png",
      "resizeMode": "contain",
      "backgroundColor": "#060313"
    },
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": false,
      "bundleIdentifier": "com.novaai.novaremote",
      "buildNumber": "1.1.0",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSFaceIDUsageDescription": "NovaRemote uses Face ID to protect your saved server credentials."
      }
    },
    "android": {
      "package": "com.novaai.novaremote",
      "adaptiveIcon": {
        "backgroundColor": "#060313",
        "foregroundImage": "./assets/novaai-logo-user.png",
        "backgroundImage": "./assets/novaai-logo-user.png",
        "monochromeImage": "./assets/novaai-logo-user.png"
      },
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/novaai-logo-user.png"
    },
    "plugins": [
      "expo-local-authentication",
      "expo-notifications",
      "react-native-purchases"
    ]
  }
}
